{
  "name": "renovate-test-nodejs",
  "version": "1.0.0",
  "description": "Sample Node.js application for testing Renovate Runner integration",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:unit": "jest --testMatch='**/tests/unit/**/*.test.js'",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.js'",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "security-audit": "npm audit --audit-level=moderate",
    "docker:build": "docker build -t renovate-test-nodejs .",
    "docker:run": "docker run -p 3000:3000 renovate-test-nodejs"
  },
  "dependencies": {
    "express": "^4.17.1",
    "cors": "^2.8.5", 
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.20",
    "axios": "^0.21.1",
    "jsonwebtoken": "^8.5.1",
    "bcryptjs": "^2.4.3",
    "validator": "^13.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "jest": "^26.6.0",
    "supertest": "^6.1.3",
    "eslint": "^7.32.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "keywords": [
    "renovate",
    "gitlab",
    "node.js",
    "express",
    "testing"
  ],
  "author": "GitLab Lab Tutorial",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.example.com/group/renovate-test-nodejs.git"
  }
}